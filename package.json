{
  "name": "auto-translate-json-library",
  "version": "1.2.5",
  "description": "Automatically translates JSON language files to other languages using Google Translate,AWS,Azure,DeepL or OpenAI",
  "main": "./index.js",
  "bin": {
    "atj": "./bin/cli.js"
  },
  "types": "./index.d.ts",
  "scripts": {
    "prepare": "tsc",
    "build": "tsc",
    "lint": "biome check ./src",
    "lint-fix": "biome check ./src --apply",
    "lint-fix-all": "biome check --apply-unsafe ./src",
    "format": "biome format ./src",
    "format-fix": "biome format ./src --write",
    "start": "ts-node src/index.ts"
  },
  "keywords": [
    "translate",
    "language",
    "json",
    "flutter",
    "angular",
    "ngx-translate",
    "transloco",
    "auto-translate-json",
    "google",
    "Azure",
    "DeepL",
    "OpenAI"
  ],
  "author": "codegrue",
  "repository": {
    "type": "git",
    "url": "https://github.com/topce/auto-translate-json-library"
  },
  "contributors": [
    "codegrue",
    "gajo357",
    "topce",
    "BenGMiles",
    "stonega"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.2.2",
    "@topce/typescript": "5.3.1",
    "@types/minimist": "1.2.2",
    "@types/node": "20.6.5",
    "@types/yargs": "17.0.25",
    "ts-node": "10.9.1"
  },
  "dependencies": {
    "@aws-sdk/client-translate": "3.418.0",
    "@google-cloud/translate": "8.0.2",
    "axios": "1.5.0",
    "dotenv": "^16.3.1",
    "form-data": "4.0.0",
    "minimist": "1.2.8",
    "openai": "4.10.0",
    "picocolors": "1.0.0",
    "uuid": "9.0.1",
    "yargs": "17.7.2"
  }
}
